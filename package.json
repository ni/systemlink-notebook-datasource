  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/data": "latest",
    "@grafana/toolkit": "latest",
    "@grafana/ui": "latest",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "5.4.0",
    "@testing-library/react": "^10.0.2",
    "@types/lodash": "latest",
    "semantic-release": "^19.0.2"
  },
  "engines": {
    "node": ">=14"
  }
}
